<?xml version="1.0" encoding="utf-8"?>
<actions>
    <action intentName="actions.intent.OPEN_APP_FEATURE">
        <fulfillment urlTemplate="https://muhasib.aliascreation.com/featured{?feature}">
            <parameter-mapping
                intentParameter="feature"
                urlParameter="feature" />
        </fulfillment>

        <parameter name="feature">
            <entity-set-reference
                entitySetId="FeatureEntitySet" />

            <pattern value="featured accounts" />
            <pattern value="show me featured accounts" />
            <pattern value="open featured accounts" />
        </parameter>
    </action>
</actions>